  "license": "MIT",
  "dependencies": {
    "underscore": "^1.8.3",
    "victor": "^1.1.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "nyc": "^13.1.0",
    "parcel-bundler": "^1.10.3",
    "standard": "^12.0.1"
  },
  "ava": {
    "files": [
      "test/**/*.spec.js"
    ]
